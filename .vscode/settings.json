{
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "json.schemas": [
    {
      "fileMatch": [
        "tsconfig.*.json"
      ],
      "url": "http://json.schemastore.org/tsconfig"
    }
  ],
  "search.exclude": {
    "dist-es5": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.enable": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "cSpell.words": [
    "greenkeeper",
    "macos",
    "pnpify",
    "yarnpkg"
  ]
}
