{
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.quickSuggestions": {
			"strings": true
		},
		"editor.suggest.insertMode": "replace"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.quickSuggestions": {
			"strings": true
		},
		"editor.suggest.insertMode": "replace"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.formatOnSave": true,
	"json.schemas": [
		{
			"fileMatch": ["tsconfig.*.json"],
			"url": "http://json.schemastore.org/tsconfig"
		}
	],
	"search.exclude": {
		"dist-es5": true,
		"**/node_modules": true,
		"**/bower_components": true,
		"**/.yarn": true,
		"**/.pnp.*": true
	},
	"eslint.enable": true,
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"cSpell.words": [
		"binrc",
		"clibuilder",
		"clirc",
		"codeql",
		"deno",
		"greenkeeper",
		"komondor",
		"libm",
		"macos",
		"miku",
		"oobm",
		"oonm",
		"oosm",
		"pinst",
		"pjson",
		"pnpify",
		"primordials",
		"repobuddy",
		"snyk",
		"tersify",
		"tsbuildinfo",
		"USERPROFILE",
		"vocaloid",
		"yarnpkg"
	]
}
