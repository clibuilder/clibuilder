{
  "name": "clibuilder",
  "version": "7.2.1",
  "description": "A CLI building library",
  "keywords": [
    "builder",
    "cli",
    "command",
    "parser"
  ],
  "homepage": "https://github.com/unional/clibuilder",
  "bugs": {
    "url": "https://github.com/unional/clibuilder/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/clibuilder.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com",
    "url": "https://github.com/unional"
  },
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./libm/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib",
    "libm",
    "ts"
  ],
  "scripts": {
    "build": "run-p build:libm build:lib",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:libm": "tsc -p tsconfig.libm.json",
    "build:watch": "tsc --watch",
    "clean": "rimraf .nyc_output .ts coverage lib libm",
    "coverage": "jest --coverage --maxWorkers=60%",
    "depcheck": "depcheck",
    "lint": "eslint --ext=js,ts .",
    "nuke": "run-s clean && rimraf node_modules",
    "test": "jest --maxWorkers=60%",
    "test:watch": "jest --watch --maxWorkers=60%",
    "verify": "npm-run-all clean -p build lint coverage depcheck",
    "watch": "jest --watch --maxWorkers=60%"
  },
  "dependencies": {
    "@types/node": "^14.18.27",
    "chalk": "^4.1.2",
    "find-installed-packages": "^2.1.6",
    "find-up": "^5.0.0",
    "global-store": "1.0.0-beta.21",
    "pad-right": "^0.2.2",
    "search-packages": "^2.1.0",
    "standard-log": "^7.1.0",
    "standard-log-color": "^8.0.0",
    "tslib": "^2.4.0",
    "type-plus": "^4.9.1",
    "wordwrap": "^1.0.0",
    "zod": "^3.17.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.0",
    "@types/wordwrap": "github:types/wordwrap",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "assertron": "^10.0.0",
    "babel-jest": "^29.0.2",
    "clibuilder-plugin-dummy": "^1.0.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-harmony": "^7.1.0",
    "jest": "^29.0.2",
    "jest-validate": "^29.0.2",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "satisfier": "^5.2.2",
    "tersify": "^3.10.2",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">= 8"
  }
}
