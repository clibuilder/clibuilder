{
  "name": "clitester",
  "version": "0.0.0",
  "private": true,
  "description": "Test CLI application output against baselines.",
  "homepage": "https://github.com/unional/clibuilder/tree/main/packages/clitester",
  "bugs": {
    "url": "https://github.com/unional/clibuilder/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/clibuilder.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com",
    "url": "https://github.com/unional"
  },
  "sideEffects": false,
  "type": "module",
  "exports": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "esm",
    "ts",
    "!*.spec.*"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf .nyc_output .ts coverage lib libm esm",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage --maxWorkers=60%",
    "depcheck": "depcheck",
    "lint": "eslint --ext=js,ts .",
    "nuke": "pnpm clean && rimraf node_modules",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --maxWorkers=60%",
    "test:watch": "jest --watch --maxWorkers=60%",
    "verify": "npm-run-all clean -p build lint coverage depcheck",
    "watch": "cross-env NODE_OPTIONS=--experimental-vm-modules pnpm test:watch"
  },
  "dependencies": {
    "clibuilder": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@types/jest": "^28.1.7",
    "@types/node": "^16.11.49",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-jest": "^28.1.3",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-harmony": "^7.1.0",
    "jest": "^28.1.3",
    "jest-validate": "^28.1.3",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  }
}
