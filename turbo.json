{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "ts/*.ts",
        "ts/*.tsx",
        "ts/**/*.ts",
        "ts/**/*.tsx",
        "!ts/*.spec.ts",
        "!ts/*.spec.tsx",
        "!ts/**/*.spec.ts",
        "!ts/**/*.spec.tsx",
        "!ts/*.stories.ts",
        "!ts/*.stories.tsx",
        "!ts/**/*.stories.ts",
        "!ts/**/*.stories.tsx"
      ],
      "outputs": [
        "lib/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "coverage": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "ts/*.ts",
        "ts/*.tsx",
        "ts/**/*.ts",
        "ts/**/*.tsx"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "depcheck": {},
    "dev": {
      "cache": false
    },
    "lint": {},
    "size": {
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "ts/*.ts",
        "ts/*.tsx",
        "ts/**/*.ts",
        "ts/**/*.tsx"
      ]
    },
    "verify": {
      "cache": false
    },
    "watch": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "ts/*.ts",
        "ts/*.tsx",
        "ts/**/*.ts",
        "ts/**/*.tsx"
      ]
    }
  }
}
