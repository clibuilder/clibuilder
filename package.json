{
  "name": "clibuilder",
  "version": "0.0.0-development",
  "description": "A CLI building library",
  "keywords": [
    "builder",
    "cli",
    "command",
    "parser"
  ],
  "homepage": "https://github.com/unional/clibuilder#readme",
  "bugs": {
    "url": "https://github.com/unional/clibuilder/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/clibuilder.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com",
    "url": "https://github.com/unional"
  },
  "files": [
    "dist-es5",
    "dist-es2015"
  ],
  "main": "dist-es5/index.js",
  "module": "dist-es2015/index.js",
  "typings": "dist-es5/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build-es5 && npm run build-es2015",
    "build-es2015": "tsc -p tsconfig.es2015.json",
    "build-es5": "tsc -p tsconfig.es5.json",
    "clean": "rimraf dist-es5 dist-es2015",
    "coverage": "jest --coverage",
    "dc": "npm run dependency-check",
    "debug": "node scripts/debug.js",
    "dependency-check": "dependency-check . --unused --no-dev -i @types/inquirer && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "verify": "npm run lint && npm run build && npm run coverage && npm run dependency-check",
    "verify-ci": "npm run lint && npm run build && jest --maxWorkers=2 --coverage --config jest.config.ci.json && npm run dependency-check",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@types/inquirer": "0.0.43",
    "@unional/logging": "^0.5.3",
    "aurelia-logging-color": "^0.5.16",
    "camel-case": "^3.0.0",
    "find-up": "^3.0.0",
    "inquirer": "^6.2.1",
    "lodash": "^4.17.11",
    "pad-right": "^0.2.2",
    "wordwrap": "^1.0.0",
    "yargs-parser": "^11.1.1"
  },
  "devDependencies": {
    "@types/find-up": "^2.1.1",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.12",
    "@types/wordwrap": "github:types/wordwrap",
    "@unional/jest-progress-reporter": "^2.0.0",
    "assertron": "^5.2.2",
    "clibuilder-plugin-dummy": "^1.0.1",
    "codecov": "^3.1.0",
    "dependency-check": "^3.2.1",
    "greenkeeper-lockfile": "^1.15.1",
    "jest": "^23.6.0",
    "jest-audio-reporter": "^2.2.0",
    "jest-junit": "^5.2.0",
    "jest-watch-suspend": "^1.1.0",
    "jest-watch-toggle-config": "^1.0.2",
    "pascal-case": "^2.0.1",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.12.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-unional": "^0.10.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.2.1"
  }
}
