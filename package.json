{
  "name": "clibuilder",
  "version": "0.1.0",
  "description": "A CLI building library",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "typings.json"
  ],
  "dependencies": {
    "camel-case": "^3.0.0",
    "minimist": "^1.2.0",
    "wordwrap": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "dependency-check": "^2.6.0",
    "rimraf": "^2.5.4",
    "tslint": "^3.14.0",
    "tslint-config-unional": "^0.4.4",
    "typescript": "^2.0.0",
    "typings": "^1.3.2"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "rimraf dist && tsc && ava \"dist/**/*.spec.js\"",
    "watch": "rimraf dist && node scripts/watch.js dist/**/*.spec.js",
    "verify": "npm run test+lint && npm run build",
    "test+lint": "npm test && npm run lint",
    "preversion": "npm run verify",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unional/clibuilder.git"
  },
  "keywords": [
    "cli",
    "builder",
    "command",
    "parser"
  ],
  "author": "Unional <homawong@gmail.com> (http://github.com/unional)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unional/clibuilder/issues"
  },
  "homepage": "https://github.com/unional/clibuilder#readme"
}
