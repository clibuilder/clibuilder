{
  "name": "clibuilder",
  "description": "A CLI building library",
  "version": "0.0.0-development",
  "homepage": "https://github.com/unional/clibuilder",
  "keywords": [
    "builder",
    "cli",
    "command",
    "parser"
  ],
  "bugs": {
    "url": "https://github.com/unional/clibuilder/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/clibuilder.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com",
    "url": "https://github.com/unional"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf lib",
    "dc": "npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev -i @types/inquirer && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "verify": "npm run lint && npm run build && npm run dependency-check && jest --coverage",
    "verify:ci": "npm run lint && npm run build && npm run dependency-check && jest --runInBand --coverage",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@types/inquirer": "0.0.43",
    "@unional/logging": "^1.0.1",
    "aurelia-logging-color": "^1.0.0",
    "camel-case": "^3.0.0",
    "find-up": "^3.0.0",
    "inquirer": "^6.2.1",
    "lodash": "^4.17.11",
    "pad-right": "^0.2.2",
    "type-plus": "^1.4.0",
    "wordwrap": "^1.0.0",
    "yargs-parser": "^12.0.0"
  },
  "devDependencies": {
    "@types/find-up": "^2.1.1",
    "@types/lodash": "^4.14.119",
    "@types/wordwrap": "github:types/wordwrap",
    "@types/yargs-parser": "^11.0.2",
    "@unional/devpkg-node": "^1.1.6",
    "assertron": "^6.0.0",
    "clibuilder-plugin-dummy": "^1.0.1"
  }
}
