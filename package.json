{
  "name": "clibuilder",
  "version": "4.1.1",
  "description": "A CLI building library",
  "keywords": [
    "builder",
    "cli",
    "command",
    "parser"
  ],
  "homepage": "https://github.com/unional/clibuilder",
  "bugs": {
    "url": "https://github.com/unional/clibuilder/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/clibuilder.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com",
    "url": "https://github.com/unional"
  },
  "files": [
    "lib",
    "libm",
    "src"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "yarn build:esnext && yarn build:es5",
    "build:es5": "tsc -p tsconfig.es5.json",
    "build:esnext": "tsc -p tsconfig.esnext.json",
    "build:watch": "tsc --watch",
    "clean": "rimraf .nyc_output .ts coverage lib libm",
    "coverage": "jest --coverage",
    "depcheck": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev -i @types/inquirer -i standard-log-color && dependency-check . --missing --no-dev",
    "lint": "eslint \"src/**/*.ts\"",
    "nuke": "yarn clean && rimraf node_modules",
    "test": "jest",
    "test:watch": "jest --watch",
    "verify": "yarn lint && yarn build && yarn dependency-check && yarn coverage",
    "watch": "yarn test:watch"
  },
  "dependencies": {
    "@types/inquirer": "^6.5.0",
    "camel-case": "^3.0.0",
    "find-installed-packages": "^2.0.1",
    "find-up": "^4.1.0",
    "inquirer": "^7.0.1",
    "lodash": "^4.17.15",
    "make-error": "^1.3.5",
    "pad-right": "^0.2.2",
    "search-packages": "^2.0.0",
    "standard-log": "^3.1.3",
    "standard-log-color": "^1.5.6",
    "type-plus": "^1.29.0",
    "wordwrap": "^1.0.0",
    "yargs-parser": "^10.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.137",
    "@types/wordwrap": "github:types/wordwrap",
    "@types/yargs-parser": "^11.0.3",
    "@unional/devpkg-node": "^1.4.0",
    "assertron": "^7.1.2",
    "chalk": "^2.4.2",
    "clibuilder-plugin-dummy": "^1.0.1"
  }
}
