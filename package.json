{
  "name": "clibuilder",
  "version": "0.0.0-development",
  "description": "A CLI building library",
  "main": "dist-es5/index.js",
  "module": "dist-es2015/index.js",
  "typings": "dist-es5/index.d.ts",
  "files": [
    "dist-es5",
    "dist-es2015"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-es5 && npm run build-es2015",
    "build-es2015": "tsc -p tsconfig.es2015.json",
    "build-es5": "tsc -p tsconfig.es5.json",
    "clean": "rimraf dist-es5 dist-es2015",
    "codecov": "codecov",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dc": "npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev -i @types/inquirer && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "verify": "npm run build && npm run lint && npm run coverage && npm run dependency-check",
    "test": "jest",
    "watch": "npm run clean && node scripts/watch.js",
    "debug": "node scripts/debug.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/clibuilder.git"
  },
  "keywords": [
    "cli",
    "builder",
    "command",
    "parser"
  ],
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com",
    "url": "https://github.com/unional"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unional/clibuilder/issues"
  },
  "homepage": "https://github.com/unional/clibuilder#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": ".*.spec.ts$",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "dependencies": {
    "@types/inquirer": "0.0.36",
    "@unional/logging": "^0.5.0",
    "aurelia-logging-color": "^0.5.15",
    "camel-case": "^3.0.0",
    "find-up": "^2.1.0",
    "inquirer": "^5.2.0",
    "lodash": "^4.17.10",
    "pad-right": "^0.2.2",
    "wordwrap": "^1.0.0",
    "yargs-parser": "^9.0.2"
  },
  "devDependencies": {
    "@types/find-up": "^2.1.1",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/node": "^9.6.7",
    "@types/wordwrap": "github:types/wordwrap",
    "assertron": "^5.1.0",
    "clibuilder-plugin-dummy": "^1.0.1",
    "dependency-check": "^3.1.0",
    "jest": "^22.4.3",
    "pascal-case": "^2.0.1",
    "power-assert": "^1.5.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.1.8",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "tslint-config-unional": "^0.9.3",
    "typescript": "^2.8.3"
  }
}
